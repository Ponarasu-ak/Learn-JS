// for.each
//https://youtu.be/hP0Rl5euD_o?si=uk38-xsAp5jEPqyB
let a = [2,5,4,8,3,4]
a.forEach((add,i,a) =>
{
    add = add+1
    console.log(add,i,a);
})

let a1 = [3, 5, 7, 2, 9, 4, 8]
a1.forEach((x, i) => { console.log(i, x); })

let b = [5, 6, 7, 8, 2]
let even = b.filter((x) => {
    let y = x % 2 == 0
    return y
}).map((x) => {
    let z = x * 2
    return z
}).every((x)=>{
    let cc = x>10
return cc})
console.log(even);

